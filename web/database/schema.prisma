datasource db {
  provider = "postgresql"
  url      = "postgresql://mxstbr@localhost:5432?schema=public"
}

generator photonjs {
  provider = "photonjs"
  output = "./generated/photon"
}

model User {
  id String @id @unique @default(cuid())
  name String
  googleId String @unique
  email String?
  avatarUrl String?
}
